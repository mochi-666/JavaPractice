package 練習;
  public class java8 {
    public static void main(String[] args) {
    	int num = (int)(Math.random()*10)+1;
    	  System.out.println(num);
    	if (num == 1) {
    	  System.out.println("１です");
    	} else if (num < 1){
    		System.out.println("1ではありません");
    	}
    }
}
