package 練習;

public class java11 {
	public static void main(String[] args) {
	  int i =(int)(Math.random()*100)+1;
	  System.out.println(i);
	  if (i >= 20 && i < 80) {
	  System.out.println("20以上８０未満です");
	  }  else if (i < 20 && i >= 80){
	     System.out.println("20未満８０以上です");
	  }
	}
}
